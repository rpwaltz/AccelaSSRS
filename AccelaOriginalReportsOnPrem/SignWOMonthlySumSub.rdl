  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":startdate">
            <Value>=Parameters!startdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name=":enddate">
            <Value>=Parameters!enddate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
DECODE(A.VALUE_, 'TNPS', 3, 'KPD/311 Investigation',4,'Road Closures',5, 1) SECTION,
a.VALUE_ MUTCDTYPE, b.VALUE_ Action,
XXCOK.COK_GET_SIGN_TYPE_ACTION_COUNT(a.VALUE_, b.VALUE_,
   :startdate,:enddate) Quantity
FROM
Accela.v_standard_choice a,
Accela.v_standard_choice b
where
A.ITEM_NAME = 'COK_MUTCD_TYPE' and
b.ITEM_NAME = 'COK_SIGN_ACTIONS' 
UNION
select 2 SECTION, 'Posts' MUTCDTYPE,
b.VALUE_ Action,
XXCOK.COK_GET_SIGN_TYPE_ACTION_COUNT('Posts', b.VALUE_, 
    :startdate,:enddate) Quantity
FROM
Accela.v_standard_choice b
where
b.ITEM_NAME = 'COK_SIGN_ACTIONS'</CommandText>
        <Timeout>10</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
