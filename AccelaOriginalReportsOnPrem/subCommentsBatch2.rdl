  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":id">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select c.clobtext
from accela.b1permit a 
left outer join accela.v_related_record r
  on r.child_record_id = a.b1_alt_id
left outer join accela.b1permit ap
  on ap.b1_alt_id = r.parent_record_id
left outer join g6action b
  on b.serv_prov_code = ap.serv_prov_code
  and b.b1_per_id1 = ap.b1_per_id1
  and b.b1_per_id2 = ap.b1_per_id2
  and b.b1_per_id3 = ap.b1_per_id3
left outer join accela.bactivity_comment c
  on c.serv_prov_code = b.serv_prov_code
  and c.b1_per_id1 = b.b1_per_id1 
  and c.b1_per_id2 = b.b1_per_id2 
  and c.b1_per_id3 = b.b1_per_id3 
  and c.g6_act_num = b.g6_act_num 
  and (c.comment_type = 'Inspection Result Comment') 
where  a.b1_alt_id = to_char(:id)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
