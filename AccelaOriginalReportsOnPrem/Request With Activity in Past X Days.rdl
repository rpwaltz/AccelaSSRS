  <DataSets>
    <DataSet Name="ActivityInXDays">
      <Query>
        <DataSourceName>ERFS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ForGroup">
            <Value>=Parameters!ForGroup.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@DaysBack">
            <Value>=Parameters!DaysBack.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select a.rfsid, deptid, requestedbyid, a.createdby, isfwdto, isassigneddivision,
substring(rfsdescription, 1, 200) as rfsdescription,
FORMAT (createdon, 'MM/dd/yyyy') as created, 
datediff(d, createdon, getdate()) as daysold, 
FORMAT (neededby, 'MM/dd/yyyy') as needed, 
substring(neededreason, 1, 200) as neededreason,
FORMAT (a.lastupdated, 'MM/dd/yyyy') as mstrupdated,
b.lastupdatedby, FORMAT (lastupdateddate, 'MM/dd/yyyy') as actdate,
substring(activitydetails, 1, 200) as activitydetails, ltrim(rtrim(deptid)) + ' - ' + ltrim(rtrim(requestedbyid)) as reqby, ltrim(rtrim(isstatuscode)) as isstatuscode
from erfsmaster a, erfsactivity b
where a.rfsid = b.rfsid
and (
(@ForGroup = 'ALL' and isassigneddivision in ('ADMIN', 'APPS', 'TECH') ) 
OR
(@ForGroup &lt;&gt; 'ALL' and isassigneddivision = @ForGroup)
)
and b.lastupdateddate = (select max(c.lastupdateddate) from erfsactivity c where b.rfsid = c.rfsid)
and ((datediff(d, b.lastupdateddate, getdate()) &lt; (@DaysBack)) or (datediff(d, a.lastupdated, getdate()) &lt; (@DaysBack)))
order by isassigneddivision, isfwdto, daysold desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
