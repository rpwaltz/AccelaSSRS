  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT a.b1_alt_id,
a.SERV_PROV_CODE,
    a.B1_PER_ID1,
    a.B1_PER_ID2,
    a.B1_PER_ID3,
    c.B1_FNAME,
    nvl(c.B1_MNAME,' ' ) B1_MNAME,
    c.B1_LNAME,
c.b1_full_name,
c.b1_address1, c.b1_city, c.b1_state, c.b1_zip

FROM
B1PERMIT a,
BPERMIT_DETAIL aa,
ACCELA.B3CONTACT c,
ACCELA.SETDETAILS
where
a.b1_per_group = 'Permits' and
a.b1_per_type = 'Stormwater' and
a.b1_per_sub_type = 'SPAP' and
a.b1_per_category = 'NA' and
a.b1_appl_status in  ('Active', 'Expired') and
aa.serv_prov_code = a.serv_prov_code and
aa.b1_per_id1 = a.b1_per_id1 and
aa.b1_per_id2 = a.b1_per_id2 and
aa.b1_per_id3 = a.b1_per_id3 
 
  AND c.SERV_PROV_CODE (+) = a.SERV_PROV_CODE
  AND c.B1_PER_ID1 (+)     = a.B1_PER_ID1
  AND c.B1_PER_ID2 (+)     = a.B1_PER_ID2
  AND c.B1_PER_ID3 (+)     = a.B1_PER_ID3
  AND c.b1_contact_type (+)   = 'Applicant'
AND      ACCELA.SETDETAILS.SERV_PROV_CODE = a.SERV_PROV_CODE AND 
                      ACCELA.SETDETAILS.B1_PER_ID1 = a.B1_PER_ID1 AND 
                      ACCELA.SETDETAILS.B1_PER_ID2 = a.B1_PER_ID2 AND 
                      ACCELA.SETDETAILS.B1_PER_ID3 = a.B1_PER_ID3
AND ((c.B1_FNAME is not null  and c.B1_LNAME is not null ) OR c.B1_full_name is not null ) 

AND     (a.B1_ALT_ID = :ID)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
