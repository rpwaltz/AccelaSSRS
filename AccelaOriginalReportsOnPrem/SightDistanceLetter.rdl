  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     ACCELA.B1PERMIT.SERV_PROV_CODE, 
to_char(ACCELA.B3ADDRES.B1_HSE_NBR_START) ||' ' ||
TRIM(ACCELA.B3ADDRES.B1_STR_PREFIX ||' ' || ACCELA.B3ADDRES.B1_STR_DIR) as STR_PREFIX, 
TRIM(ACCELA.B3ADDRES.B1_STR_NAME ||' ' || ACCELA.B3ADDRES.B1_STR_SUFFIX) as STREET, 
ACCELA.B3ADDRES.B1_SITUS_CITY, ACCELA.B3ADDRES.B1_SITUS_STATE, ACCELA.B3ADDRES.B1_SITUS_ZIP,
ACCELA.B1PERMIT.B1_ALT_ID as WO_NUM
                       
FROM         ACCELA.B1PERMIT, ACCELA.B3ADDRES
WHERE     ACCELA.B1PERMIT.SERV_PROV_CODE = ACCELA.B3ADDRES.SERV_PROV_CODE AND ACCELA.B1PERMIT.B1_PER_ID1 = ACCELA.B3ADDRES.B1_PER_ID1 AND 
                      ACCELA.B1PERMIT.B1_PER_ID2 = ACCELA.B3ADDRES.B1_PER_ID2 AND ACCELA.B1PERMIT.B1_PER_ID3 = ACCELA.B3ADDRES.B1_PER_ID3 
                     AND 
                      (ACCELA.B1PERMIT.B1_ALT_ID = TO_CHAR(:ID))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SERV_PROV_CODE">
          <DataField>SERV_PROV_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="STR_PREFIX">
          <DataField>STR_PREFIX</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="STREET">
          <DataField>STREET</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="B1_SITUS_CITY">
          <DataField>B1_SITUS_CITY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="B1_SITUS_STATE">
          <DataField>B1_SITUS_STATE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="B1_SITUS_ZIP">
          <DataField>B1_SITUS_ZIP</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WO_NUM">
          <DataField>WO_NUM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
