  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     ACCELA.B1PERMIT.B1_ALT_ID, ACCELA.BCHCKBOX.B1_CHECKBOX_DESC, ACCELA.BCHCKBOX.B1_CHECKLIST_COMMENT AS Comments
FROM         ACCELA.B1PERMIT, ACCELA.BCHCKBOX
WHERE     ACCELA.B1PERMIT.SERV_PROV_CODE = ACCELA.BCHCKBOX.SERV_PROV_CODE AND ACCELA.B1PERMIT.B1_PER_ID1 = ACCELA.BCHCKBOX.B1_PER_ID1 AND 
                      ACCELA.B1PERMIT.B1_PER_ID2 = ACCELA.BCHCKBOX.B1_PER_ID2 AND ACCELA.B1PERMIT.B1_PER_ID3 = ACCELA.BCHCKBOX.B1_PER_ID3 AND 
                      (ACCELA.B1PERMIT.B1_PER_TYPE = 'Work Order') AND (ACCELA.BCHCKBOX.B1_CHECKBOX_DESC = 'Service Request Information') AND 
                      (ACCELA.B1PERMIT.B1_ALT_ID = TO_CHAR(:ID))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="B1_ALT_ID">
          <DataField>B1_ALT_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
