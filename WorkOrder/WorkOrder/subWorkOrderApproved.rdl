  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Accela</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ID">
            <Value>=Parameters!ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @ID     varchar(1000) = 'WO-PS-22-004783';
  select
   NULLIF(gph.SD_APP_DES,'') AS STATUS ,
   gph.REC_FUL_NAM AS UPDATED_BY ,
  gph.REC_DATE AS UPDATED_DATE
  from B1PERMIT bp
  INNER JOIN GPROCESS_HISTORY gph
    ON bp.SERV_PROV_CODE = gph.SERV_PROV_CODE
      AND bp.B1_PER_ID1 = gph.B1_PER_ID1
      AND bp.B1_PER_ID2 = gph.B1_PER_ID2
      AND bp.B1_PER_ID3 = gph.B1_PER_ID3
  -- since the same individual may approve the permit multiple times, we should only display the first time an individual signs
  INNER JOIN 
  (  SELECT	gph.REC_FUL_NAM,
			min(GPROCESS_HISTORY_SEQ_NBR) as GPROCESS_HISTORY_SEQ_NBR
		FROM GPROCESS_HISTORY gph
		inner join B1PERMIT bp
		on bp.SERV_PROV_CODE = gph.SERV_PROV_CODE
		AND bp.B1_PER_ID1 = gph.B1_PER_ID1
		AND bp.B1_PER_ID2 = gph.B1_PER_ID2
        AND bp.B1_PER_ID3 = gph.B1_PER_ID3
		where bp.B1_ALT_ID = @ID
		group by gph.REC_FUL_NAM
		) gphg on gph.GPROCESS_HISTORY_SEQ_NBR = gphg.GPROCESS_HISTORY_SEQ_NBR
  LEFT JOIN G3DPTTYP g3t
    ON gph.SERV_PROV_CODE = g3t.SERV_PROV_CODE
      AND gph.SD_AGENCY_CODE = g3t.R3_AGENCY_CODE
      AND gph.SD_BUREAU_CODE = g3t.R3_BUREAU_CODE
      AND gph.SD_DIVISION_CODE = g3t.R3_DIVISION_CODE
      AND gph.SD_SECTION_CODE = g3t.R3_SECTION_CODE
      AND gph.SD_GROUP_CODE = g3t.R3_GROUP_CODE
      AND gph.SD_OFFICE_CODE = g3t.R3_OFFICE_CODE
	where bp.SERV_PROV_CODE ='KNOXVILLE'
		AND bp.B1_ALT_ID = @ID
	    AND NULLIF(g3t.R3_DEPTNAME,'') in ('Stormwater', 'Public Service Work Order') 
		AND NULLIF(gph.R1_PROCESS_CODE,'')  in ('WO_SW', 'WO_PUB_SER') 
		AND gph.SD_PRO_DES LIKE '%Setup Work Order%' 
		AND NULLIF(gph.SD_APP_DES,'')  in ('Approved', 'SW Approved') 
	ORDER BY UPDATED_DATE</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
